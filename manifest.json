{
    "name": "dropbox-tile",
    "version": "0.1",
    "description": "dropbox-tile",
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "permissions": [
        "https://api.dropbox.com/"
    ],
    "options_page": "options.html",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://api.dropbox.com; object-src 'self'"
}
